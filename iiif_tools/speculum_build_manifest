#!/usr/bin/env python

"""Usage:
   speculum_build_manifest <identifier>
"""

from docopt import docopt
import json
import re
import requests
import sys
import urllib.parse
import uuid

from io import BytesIO
from PIL import Image
Image.MAX_IMAGE_PIXELS = 2000000000

class SpeculumIIIFManifest:
    """Make a manifest for one of the speculum documents.
       e.g.  https://iiif-manifest.lib.uchicago.edu/speculum/0001/speculum-0001
    """

    def __init__(self, identifier, ark, title, description, attribution, metadata):
        self.identifier = identifier
        self.ark = ark
        self.title = title
        self.description = description
        self.attribution = attribution
        self.metadata = metadata

        self.directory = '/data/digital_collections/ark_data'

        noid = ark.split('/').pop()

        with open('{}/{}/inventory.json'.format(
            self.directory,
            '/'.join([noid[c:c+2] for c in range(0, len(noid), 2)])
        )) as f:
            inventory = json.load(f)

        self.sizes = []
        with BytesIO(
            requests.get('http://ark.lib.uchicago.edu/{}/file.tif'.format(
                self.ark
            )).content
        ) as f:
            self.sizes.append(Image.open(f).size)

    def _get_manifest_url(self):
       return 'https://iiif-manifest.lib.uchicago.edu/{}/{}/{}.json'.format(
           self.identifier.split('-')[0],
           self.identifier.split('-')[1],
           self.identifier
       )

    def get_width(self, n):
        return self.sizes[n][0]
            
    def get_height(self, n):
        return self.sizes[n][1]

    def get_imageserver_url(self, n):
        return 'https://iiif-server.lib.uchicago.edu/{}'.format(
            urllib.parse.quote(self.ark, safe='')
        )

    def data(self):
        manifest = {
            '@context': 'http://iiif.io/api/presentation/2/context.json',
            '@id': self._get_manifest_url(),
            '@type': 'sc:Manifest',
            'metadata': self.metadata,
            'description': self.description,
            'logo': 'https://www.lib.uchicago.edu/static/base/images/color-logo.png',
            'license': 'https://creativecommons.org/licenses/by-nc/4.0/',
            'attribution': self.attribution,
            'label': self.title,
            'sequences': [
                {
                    '@id': 'https://{}'.format(str(uuid.uuid1())),
                    '@type': 'sc:Sequence',
                    'canvases': [],
                    'viewingHint': 'non-paged'
                }
            ],
            'structures': []
        }

        manifest['sequences'][0]['canvases'].append({
            '@id': 'https://{}'.format(str(uuid.uuid1())),
            '@type': 'sc:Canvas',
            'label': 'Image {:03d}'.format(1),
            'height': self.get_height(0),
            'width': self.get_width(0),
            'images': [
                {
                    '@context': 'http://iiif.io/api/presentation/2/context.json',
                    '@id': 'https://{}'.format(uuid.uuid1()),
                    '@type': 'oa:Annotation',
                    'motivation': 'sc:Painting',
                    'resource': {
                     '@id': 'https://{}'.format(uuid.uuid1()),
                     '@type': 'dctypes:Image',
                     'format': 'image/jpeg',
                     'height': self.get_height(0),
                     'width': self.get_width(0),
                     'service': {
                         '@context': 'http://iiif.io/api/image/2/context.json',
                         '@id': self.get_imageserver_url(0),
                         'profile': [
                             'http://iiif.io/api/image/2/level2.json',
                             {
                                 'supports': [
                                     'canonicalLinkHeader',
                                     'profileLinkHeader',
                                     'mirroring',
                                     'rotationArbitrary',
                                     'regionSquare',
                                     'sizeAboveFull'
                                 ],
                                 'qualities': [
                                     'default',
                                     'gray',
                                     'bitonal'
                                 ],
                                 'format': [
                                     'jpg',
                                     'png',
                                     'gif',
                                     'webp'
                                 ]
                             }
                         ]
                     }
                    },
                    'on': 'https://{}'.format(uuid.uuid1())
                }
            ]
        })

        return manifest


