    with open('metadata/speculum.json') as f:
        metadata = json.load(f)

    title = None
    for m in metadata[arguments['<identifier>']]:
        if m['label'] == 'Title':
            title = m['value']
    assert title is not None

    print(
        json.dumps(
            SpeculumIIIFManifest(
                arguments['<identifier>'],
                arks[arguments['<identifier>']],
                title,
                'Speculum Romanae Magnificentiae from the University of Chicago.',
                'University of Chicago Library',
                metadata[arguments['<identifier>']] 
            ).data(),
